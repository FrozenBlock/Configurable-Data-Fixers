{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "Configurable Data Fixers",
  "description": "Provides a config for converting in-game registered IDs.",
  "authors": [
    "FrozenBlock"
  ],
  "contributors": [
    "Treetrain1"
  ],
  "contact": {
    "homepage": "https://github.com/FrozenBlock/Configurable-Data-Fixers",
    "sources": "https://github.com/FrozenBlock/Configurable-Data-Fixers",
    "issues": "https://github.com/FrozenBlock/Configurable-Data-Fixers"
  },

  "license": "LGPL-3.0",
  "icon": "assets/configurabledatafixers/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.frozenblock.configurabledatafixers.ConfigurableDataFixers"
    ]
  },
  "mixins": [
    "configurabledatafixers.mixins.json"
  ],
  "accessWidener": "${mod_id}.accesswidener",

  "depends": {
    "frozenlib": ">=1.1.11-",
    "fabricloader": ">=0.14.12",
    "fabric-api": ">=0.73.0",
    "minecraft": ">=${minecraft_version}",
    "java": ">=17"
  },
  "breaks": {
    "quilted_fabric_api": "4.0.0-beta.8"
  },
  "suggests": {
    "cloth-config": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/EpkBWm844s"
      },
      "badges": [
        "frozenblock"
      ]
    }
  }
}
